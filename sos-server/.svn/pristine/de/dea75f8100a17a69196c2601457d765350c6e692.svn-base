class BuriednessHumanComparator implements Comparator<Human> {

	@Override
	public int compare(Human h1, Human h2) {
		if (h1.getBuriedness() < h2.getBuriedness())
			return 1;
		if (h1.getBuriedness() > h2.getBuriedness())
			return -1;
		return 0;
	}
}


